{
  "name": "auto-image-path-path-updater",
  "displayName": "Auto Image Path Updater",
  "description": "Auto Image Path Updater simplifies your workflow by automatically updating image file paths in HTML,CSS,Javascript and JSON files when images are moved within your workspace. This tool ensures that all references to images are accurate and up-to-date, saving you time and reducing errors.",
  "publisher": "DeepakKN",
  "version": "0.0.1",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepakCSGO23/Auto-Image-Path-Updater-VSCode-Extension"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "auto-path-renamer.helloWorld",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^9.9.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
